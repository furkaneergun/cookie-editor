function BrowserDetector(){"use strict";let e=chrome||window.browser||window.chrome,t,r=null;if(e===chrome||e===window.chrome){let i=!1;try{i=e.runtime.getPlatformInfo() instanceof Promise}catch(o){}t=i?"safari":"chrome"}else if(e===window.browser){let n=!1;try{n=e.runtime.getPlatformInfo() instanceof Promise}catch(s){}t=n?"firefox":"edge"}this.getApi=function(){return e},this.isFirefox=function(){return"firefox"===t},this.isChrome=function(){return"chrome"===t},this.isEdge=function(){return"edge"===t},this.isSafari=function(){return"safari"===t},this.supportSameSiteCookie=function(){if(null!==r)return r;try{this.getApi().cookies.set({url:"https://example.com/",name:"testSameSite",value:"someValue",sameSite:"strict"},e=>{let t=this.getApi().runtime.lastError;if(!e||t){console.error("Failed to create cookie",t),r=!1;return}r=!0})}catch(e){r=!1}return r},this.supportSameSiteCookie()}